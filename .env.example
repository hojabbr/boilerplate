# Copy this file to .env, then run: php artisan key:generate
# Configure APP_*, DB_*, and optionally MEILISEARCH_*, REVERB_*, etc. as needed for your environment.

APP_NAME=APPLICATION
APP_DESCRIPTION="Build something great."
APP_ENV=local
# Run `php artisan key:generate` after copying to .env
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=20

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

